      <Button fx:id="btnPintura" layoutX="132.0" layoutY="258.0" mnemonicParsing="false" onMouseClicked="#switchToSecondary" prefHeight="42.0" prefWidth="252.0" text="Pintura" AnchorPane.leftAnchor="132.0">
